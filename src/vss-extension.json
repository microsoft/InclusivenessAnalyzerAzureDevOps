{
    "manifestVersion": 1,
    "id": "InclusivenessAnalyzerAzureDevOps",
    "name": "Inclusiveness Analyzer",
    "version": "0.0.7",
    "publisher": "InclusivenessAnalyzer",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Make your code inclusive! This extension checks code and docs on every commit for offensive and exclusive terms like blacklist and suggests alternatives.",
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "inclusive",
        "d&i",
        "inclusion"
    ],
    "galleryFlags": [
        "Public"
    ],
    "links": {
        "home": {
            "uri": "https://microsoft.github.io/InclusivenessAnalyzer/"
        },
        "getstarted": {
            "uri": "https://microsoft.github.io/InclusivenessAnalyzer/azuredevopsextension"
        },
        "learn": {
            "uri": "https://microsoft.github.io/InclusivenessAnalyzer/azuredevopsextension"
        },
        "support": {
            "uri": "https://github.com/microsoft/InclusivenessAnalyzerAzureDevOps/issues"
        },
        "repository": {
            "uri": "https://github.com/microsoft/InclusivenessAnalyzerAzureDevOps"
        },
        "issues": {
            "uri": "https://github.com/microsoft/InclusivenessAnalyzerAzureDevOps/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/microsoft/InclusivenessAnalyzerAzureDevOps"
    },
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "content": {
        "details": {
            "path": "docs/overview.md"
        }
    },
    "files": [
        {
            "path": "InclusivenessAnalyzer"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "InclusivenessAnalyzer",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "InclusivenessAnalyzer"
            }
        }
    ]
}