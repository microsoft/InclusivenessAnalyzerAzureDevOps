    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "InclusivenessAnalyzer"
        }
    ],
    "contributions": [
        {
            "id": "InclusivenessAnalyzer",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "InclusivenessAnalyzer"
            }
        }
    ]
}