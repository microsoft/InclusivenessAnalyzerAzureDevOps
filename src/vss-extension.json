{
    "manifestVersion": 1,
    "id": "InclusivenessAnalyzerAzureDevOps",
    "name": "Inclusiveness Analyzer",
    "version": "0.0.2",
    "publisher": "InclusivenessAnalyzer",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Azure DevOps extension that checks code and docs for offensive / exclusive terms and provides warnings.",
    "categories": [
        "Azure Pipelines"
    ],
    "galleryFlags": [
        "Preview"
